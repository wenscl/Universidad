    <connection>Conexion_Abonos</connection>
    <sql>SELECT c.codigo_cliente as codigo, c.nombre, c.apellido, ne.descripcion as nivel_estudio,
	CASE WHEN (c.sexo = 1) THEN &apos;Masculino&apos; ELSE &apos;Femenino&apos; END as sexo
FROM public.cliente c
join public.nivel_estudio ne on c.cod_nivel_estudio = ne.cod_nivel_estudio</sql>
